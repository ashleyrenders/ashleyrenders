<div class="instagram">
  <span class="title">Follow me on Instagram</span>
  <div id="instafeed">
    <script type="text/javascript">
      var feed = new Instafeed({
          get: 'user',
          userId: '54552992',
          limit: '6',
          accessToken: "<%= ENV['INSTAGRAM_ACCESS_TOKEN'] %>"
      });
      feed.run();
    </script>
  </div>
</div>
